          "rawQuery": true,
          "rawSql": "SELECT fqdns.content->>'name' AS \"FQDN\", ips.content->>'address' AS \"IP Address\", nets.content->>'cidr' AS \"Netblock\", asns.content->>'number' AS \"ASN\", orgs.content->>'name' AS \"Organization\" \nFROM ((((((((assets AS fqdns INNER JOIN relations AS rels1 ON fqdns.id = rels1.from_asset_id) INNER JOIN assets AS ips ON rels1.to_asset_id = ips.id) INNER JOIN relations AS rels2 ON ips.id = rels2.to_asset_id) INNER JOIN assets AS nets ON rels2.from_asset_id = nets.id) INNER JOIN relations AS rels3 ON nets.id = rels3.to_asset_id) INNER JOIN assets AS asns ON rels3.from_asset_id = asns.id) INNER JOIN relations AS rels4 ON asns.id = rels4.from_asset_id) INNER JOIN assets AS orgs ON rels4.to_asset_id = orgs.id) WHERE rels1.type IN ('a_record', 'aaaa_record') AND rels2.type = 'contains' AND fqdns.type = 'FQDN' AND ips.type = 'IPAddress' AND nets.type = 'Netblock' AND rels3.type = 'announces' AND rels4.type = 'managed_by' AND orgs.type = 'RIROrg' AND asns.type = 'ASN' AND nets.type = 'Netblock' AND rels2.last_seen > '${__from:date:iso}' AND rels2.last_seen < '${__to:date:iso}' AND rels3.last_seen > '${__from:date:iso}' AND rels3.last_seen < '${__to:date:iso}' AND rels4.last_seen > '${__from:date:iso}' AND rels4.last_seen < '${__to:date:iso}' AND fqdns.content->>'name' IN (${subs})",
